        . = ALIGN(4);
        __tcm_data_end__ = .;
        CHECK_ALIGN_4(__tcm_data_end__);
    } > DTCM AT > PROGRAM
    __tcm_data_size__ = __tcm_data_end__ - __tcm_data_begin__;

    .plat_bss_tcm : ALIGN(8) {
        __tcm_bss_begin__ = .;
        PLAT_TCM_NONROM(.bss*)
    } > DTCM

    .btc_bss_tcm : ALIGN(8) {
        FILL(0xFF)
        . = ALIGN(8);
        BTC_TCM_NONROM(.bss*)
    } > DTCM

    .wifi_bss_tcm : ALIGN(8) {
        WIFI_TCM_NONROM(.bss*)
        WIFI_FLASH_NONROM(.bss*)
        . = ALIGN(4);
        __tcm_bss_end__ = .;
    } > DTCM
    __tcm_bss_size__ = __tcm_bss_end__ - __tcm_bss_begin__;
}

