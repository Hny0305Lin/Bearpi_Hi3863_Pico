    .wifi_text_tcm : ALIGN(8) {
        FILL(0xFF)
        . = ALIGN(8);
        WIFI_TCM_SECTION()
        WIFI_TCM_NONROM(.text* .rodata*)
        LWIP_TCM_SECTION()
    } > ITCM AT > PROGRAM

    .patch_on_tcm : ALIGN(8) {
        FILL(0xFF)
        . = ALIGN(8);
        KEEP(*(.patch.text.on.itcm .patch.rodata.on.itcm))
